#!/bin/sh

PREREQ=""
prereqs()
{
	echo "$PREREQ"
}
case $1 in
# get pre-requisites
prereqs)
	prereqs
	exit 0
	;;
esac

# Should terminal be in UTF8 mode?
if [ -x /bin/kbd_mode ]; then
	/bin/kbd_mode -u -C /dev/console
fi

# Load custom keymap
if [ -x /bin/loadkmap -a -r /etc/bkeymap ]; then
	/bin/loadkmap < /etc/bkeymap
fi

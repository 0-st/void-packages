# Template file for 'libwmf-devel'.
#
depends="libwmf>=$version"
short_desc="${sourcepkg} development files"
long_desc="${long_desc}

 This package contains files for development, headers, static libs, etc."

do_install() {
	vmove usr/include usr
	vmove "usr/lib/*.a" usr/lib
	vmove usr/share/doc usr/share
	vmove usr/bin/libwmf-config usr/bin
	vmove "usr/lib/gtk-2.0/2.10.0/loaders/*.a" usr/lib/gtk-2.0/2.10.0/loaders
}

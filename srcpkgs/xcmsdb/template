pkgname=xcmsdb
version=1.0.2
distfiles="${XORG_SITE}/app/$pkgname-$version.tar.bz2"
build_style=gnu_configure
short_desc="Device Color Characterization utility for X"
maintainer="Juan RP <xtraeme@gmail.com>"

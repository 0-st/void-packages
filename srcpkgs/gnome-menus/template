# Template file for 'gnome-menus'
pkgname=gnome-menus
version=2.91.91
distfiles="${GNOME_SITE}/$pkgname/2.91/$pkgname-$version.tar.bz2"
build_style=gnu_configure
configure_args="--localstatedir=/var --disable-static"
short_desc="GNOME menu specifications"
maintainer="Juan RP <xtraeme@gmail.com>"
checksum=79bd6a2b5ec048d811a39567ea22d623a37daf06f05a9ed0ac340b673a639452
long_desc="
 This package provides an implementation of the Desktop Menu Specification
 for the GNOME Desktop."

subpackages="$pkgname-python $pkgname-devel"

Add_dependency run glibc
Add_dependency run glib

Add_dependency build pkg-config
Add_dependency build intltool
Add_dependency build gobject-introspection
Add_dependency build glib-devel
Add_dependency build python-devel
Add_dependency build pygtk-devel

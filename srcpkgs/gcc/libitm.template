short_desc="GCC Transactional Memory support library"
long_desc="${long_desc}

 This package contains GCC support for Transactional Memory, which is
 used when C code is built with -fgnu-tm flag."

do_install() {
	vmove "usr/lib/libitm.*" usr/lib

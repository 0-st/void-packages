	noarch=yes
	short_desc+=" - Chinese (traditional) language pack"
	pkg_install() {
		vmkdir opt
		cp -R ${wrksrc}/p_zh-TW/opt/libreoffice*/ ${PKGDESTDIR}/opt/
	}
}


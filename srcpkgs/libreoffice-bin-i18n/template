libreoffice-bin-i18n-af_package () {
	depends="libreoffice-bin"
	noarch=yes
	short_desc+=" - Afrikaans language pack"
	pkg_install() {
		install -d ${PKGDESTDIR}/opt
		cp -R ${wrksrc}/p_af/opt/libreoffice*/ ${PKGDESTDIR}/opt/
	}

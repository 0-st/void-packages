libreoffice-bin-i18n-zu_package () {
	depends="libreoffice-bin"
	noarch=yes
	short_desc+=" - Zulu language pack"
	pkg_install() {
		vmkdir opt
		cp -R ${wrksrc}/p_zu/opt/libreoffice*/ ${PKGDESTDIR}/opt/
	}
}

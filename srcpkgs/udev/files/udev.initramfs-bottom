#!/bin/sh -e
# initramfs premount script for udev

PREREQ=""

# Output pre-requisites
prereqs()
{
	echo "$PREREQ"
}

case "$1" in
    prereqs)
	prereqs
	exit 0
	;;
esac


# Kill the udev daemon.
pkill udevd

# Clean up udev state stuff.
udevadm info --cleanup-db

# Move /dev to the real filesystem
mount -n -o move /dev ${rootmnt}/dev

#
# This script removes the avahi and netdev user/group.
#
case "${ACTION}" in
pre)
	if [ "$UPDATE" = "no" ]; then
		userdel avahi &>/dev/null
		[ $? -eq 0 ] && echo "Removed avahi system user/group."
		userdel netdev &>/dev/null
		[ $? -eq 0 ] && echo "Removed netdev system user/group."
	fi
	;;
post)
	;;
esac

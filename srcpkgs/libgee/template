# Template file for 'libgee'
pkgname=libgee
version=0.6.2.1
distfiles="${GNOME_SITE}/$pkgname/0.6/$pkgname-$version.tar.xz"
build_style=gnu_configure
short_desc="GObject collection library"
maintainer="Juan RP <xtraeme@gmail.com>"

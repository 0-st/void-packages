# Common vars shared by the 'kernel' and 'kernel-libc-headers'
# build templates.
version=3.0
_kernel_ver_minor=0
wrksrc="linux-$version"
distfiles="${KERNEL_SITE}/kernel/v3.0/linux-${version}.tar.bz2"

#!/bin/sh
#
# PROVIDE: udev_events
# REQUIRE: udevd

$_rc_subr_loaded . /etc/rc.subr

name="udev_events"
start_cmd="udev_events_start"
stop_cmd=":"

udev_events_start()
{
	echo -n "Waiting for udev uevents... "
	udevadm trigger
	udevadm settle
	echo "done."
}

load_rc_config $name
run_rc_command "$1"

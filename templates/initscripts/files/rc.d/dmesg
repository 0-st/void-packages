#!/bin/sh
#
# PROVIDE: dmesg
# REQUIRE: SERVERS persistent_rules

$_rc_subr_loaded . /etc/rc.subr

name="dmesg"
start_cmd="dmesg_start"
stop_cmd=":"

dmesg_start()
{
	if [ -f /var/log/dmesg.log ]; then
        	rm -f /var/log/dmesg.log
	fi
	dmesg > /var/log/dmesg.log
}

load_rc_config $name
run_rc_command "$1"
